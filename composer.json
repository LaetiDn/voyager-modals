{
    "name": "ctrlwebinc/voyager-page-blocks-modals",
    "description": "Arrrrr! A package to implement modals into Voyager Page Blocks",
    "license": "MIT",
    "authors": [
        {
            "name": "ctrlweb",
            "email": "info@ctrlweb.ca",
            "homepage": "https://ctrlweb.ca/"
        }
    ],
    "homepage": "https://github.com/ctrlwebinc/voyager-page-blocks-modals",
    "keywords": ["Laravel", "Voyager", "Voyager Page Blocks", "Modals"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ctrlweb\\VoyagerPageBlocksModals\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ctrlweb\\VoyagerPageBlocksModals\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ctrlweb\\VoyagerPageBlocksModals\\VoyagerPageBlocksModalsServiceProvider"
            ],
            "aliases": {
                "VoyagerPageBlocksModals": "Ctrlweb\\VoyagerPageBlocksModals\\Facades\\VoyagerPageBlocksModals"
            }
        }
    }
}
