{
    "name": "ctrlwebinc/voyager-page-modals",
    "description": "Arrrrr! A package to implement modals into Voyager Pages and Voyager Page Blocks",
    "license": "MIT",
    "authors": [
        {
            "name": "ctrlweb",
            "email": "info@ctrlweb.ca",
            "homepage": "https://ctrlweb.ca/"
        }
    ],
    "homepage": "https://github.com/ctrlwebinc/voyager-page-modals",
    "keywords": ["Laravel", "Voyager", "Voyager Pages", "Voyager Page Blocks", "Modals"],
    "require": {
        "illuminate/support": "~5",
        "tcg/voyager": "^1.1",
        "pvtl/voyager-frontend": "~0.8.0",
        "pvtl/voyager-pages": "~0.2.0",
        "pvtl/voyager-page-blocks": "~0.8.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ctrlweb\\VoyagerPageModals\\": "src/",
            "Ctrlweb\\VoyagerPageModals\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ctrlweb\\VoyagerPageModals\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ctrlweb\\VoyagerPageModals\\VoyagerPageModalsServiceProvider"
            ],
            "aliases": {
                "VoyagerPageModals": "Ctrlweb\\VoyagerPageModals\\Facades\\VoyagerPageModals"
            }
        }
    }
}
